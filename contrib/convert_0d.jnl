if `nc_save` then
 define variable lwnet = inland_var
 set variable/title="`title`" lwnet
 save/append/file=inland-0d-data.nc/title="`title`" lwnet
 define variable lwnet = ibis_var
 set variable/title="`title`" lwnet
 save/append/file=ibis-0d-data.nc/title="`title`" lwnet
